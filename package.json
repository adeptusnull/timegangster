  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint src/**/*.ts",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --forceExit",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "files": [
